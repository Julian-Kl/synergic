{
    "compilerOptions": {
        "jsx": "react",
        "target": "es5",
        "module": "es6",
        "allowJs": false,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    }
}